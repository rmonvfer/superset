{
    "name": "@superset/repo",
    "version": "0.0.0",
    "description": "Superset Monorepo",
    "homepage": "https://superset.sh",
    "license": "Apache-2.0",
    "private": true,
    "type": "module",
    "workspaces": [
        "packages/*",
        "apps/*",
        "tooling/*",
        "apps/*"
    ],
    "scripts": {
        "dev": "bun --filter '*' dev",
        "test": "bun --filter '*' test",
        "db:gen": "bun --filter @superset/db db:gen",
        "db:push": "cd packages/db && bun db:push",
        "db:seed": "cd packages/db && bun db:seed",
        "db:migrate": "cd packages/db && bun db:migrate",
        "db:reset": "cd apps/backend && bun run reset",
        "format": "biome format --write .",
        "lint": "biome check .",
        "lint:fix": "biome check --write .",
        "typecheck": "bun --filter '*' typecheck",
        "clean": "git clean -xdf node_modules",
        "clean:workspaces": "bun --filter '*' clean",
        "setup:env": "cd packages/scripts && bun run start"
    },
    "repository": {
        "type": "git"
    },
    "packageManager": "bun@1.3",
    "devDependencies": {
        "@biomejs/biome": "^2.2.6"
    }
}
